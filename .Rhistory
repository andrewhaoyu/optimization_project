int          y[N];
vector[N]    w;
int          COVFN;
}
parameters{
//define prior
real           beta;
real<lower=0>  tau2;
//real<lower=0>  sigma2;
real<lower=0>  lambda;
vector[N]      m;
}
transformed parameters{
//declare mu,Sigma
vector[N]   mu;
matrix[N,N] Sigma;
vector[N]   f;
//define mu
for(i in 1:N)
mu[i] = beta*w[i];
Sigma = kxx(w, N, COVFN, lambda, tau2);
for(k in 1:N)
f[k] = normal_cdf(m[k],0,1);
}
model{
//prior
beta   ~ normal(0,1000);
lambda ~ cauchy(0, 2.5);
//sigma2 ~ lognormal(0, 2);
tau2   ~ lognormal(0, 2);
//relation between f and m, since z|m~normal(m,1)
target += multi_normal_lpdf(m | mu, Sigma);
target += binomial_lpmf(y | 1, f);
}'
