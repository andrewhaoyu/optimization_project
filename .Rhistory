stan.model <- '
functions {
real kappa_f(real x, real xprime, real lambda, int covfn) {
real d;
real tmp;
real rst;
d = (x-xprime)^2;
d = lambda * sqrt(d);
if (0 == covfn) {
rst = exp(-d^2/2);
} else if (1 == covfn) {
tmp = sqrt(3)*d;
rst = (1 + tmp) * exp(-tmp);
} else if (2 == covfn) {
tmp = sqrt(5)*d;
rst = (1 + tmp + tmp^2/3) * exp(-tmp);
}
return(rst);
}
matrix kxx(vector x, int ntot, int covfn, real lambda, real tau2) {
matrix[ntot, ntot] rst;
for (i in 1:ntot) {
# nugget 0.00001 for stability
rst[i,i] = tau2  + 0.00001;
for (j in (i+1):ntot) {
rst[i,j] = tau2 * kappa_f(x[i], x[j], lambda, covfn);
rst[j,i] = rst[i,j];
}
}
return(rst);
}
}
